{
    "rulesDirectory": [
        "tslint-config-standard",
        "tslint-plugin-prettier"
    ],
    "extends": [
        "tslint-config-prettier"
    ],
    "rules": {
        "prettier": [
            true,
            {
                "printWidth": 90,
                "singleQuote": true,
                "semi": true,
                "trailingComma": "all"
            }
        ]
    }
}